


d1 >> dirt([0,4,2,1], dur=linvar([0.25, 0.25,0.5],4), lpf=linvar([2000,6000,1000],6), lpr=P[1,0.3,1].stretch(4),blur=3,cut:[2,12,4])

d1 >> dirt([0,[1,-1],(4,2,1),4,2,1], dur=linvar([1,[0.25,0.5], 0.25,0.5],4), lpf=linvar([2000,6000,1000],6), lpr=P[1,1,1].stretch(4),blur=linvar([3,2,3,1,4],3),slide=20,cut=linvar([1,0,2,0,0],4),amp=linvar([0.7,1.25],4))

d1 >> dirt([0,[2,-2],(0,2,2),0,2,-2], dur=([1,[0.25,0.5], 0.25,0.5],4), lpf=linvar([2000,6000,500],3), lpr=P[1,1,1].stretch(2),blur=linvar([3,2,3,1,4],3),slide=linvar([2,0,1,3],2),cut=linvar([1,0,2,0,0],4),amp=linvar([0.7,1.25],4))




d2 >> play("x-x", echo=linvar([20,100]))

d1 >> dirt([0,0,2,2,[5,7,7,7],3,3,3,9,9,7,7,7], dur=([1,[0.75,0.25], 0.5,0.5],4), lpf=linvar([2000,6000,500],3), lpr=P[1,1,1].stretch(2),blur=linvar([3,2,3,1,4],3),slide=linvar([2,0,1,3],2),cut=linvar([1,0,2,0,0],4),amp=linvar([0.7,1.25],4))



d2 >> play("x-[----]-x-o", echo=linvar([20,100]),amp=[1.5,1,1])
d2.stop()

d1 >> dirt([0,0,2,2,[5,7,7,7],3,3,3,9,9,7,7,7], dur=([1,[0.75,0.25], 0.5,0.5],4), lpf=linvar([2000,6000,500],3), lpr=P[1,1,1].stretch(2),blur=linvar([3,2,3,1,4],3),slide=linvar([2,0,1,3],2),cut=linvar([1,0,2,0,0],4),amp=linvar([0.7,1.25],4))
